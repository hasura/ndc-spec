{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "QueryResponse",
  "description": "Query responses may return multiple RowSets when using foreach queries Else, there should always be exactly one RowSet",
  "type": "array",
  "items": {
    "$ref": "#/definitions/RowSet"
  },
  "definitions": {
    "Row": {
      "type": "object",
      "properties": {
        "columns": {
          "description": "Columns fetched from the requested collection",
          "type": "object",
          "additionalProperties": true
        },
        "relationships": {
          "description": "Related collections fetched via relationships",
          "type": "object",
          "additionalProperties": {
            "$ref": "#/definitions/RowSet"
          }
        }
      }
    },
    "RowSet": {
      "type": "object",
      "properties": {
        "aggregates": {
          "description": "The results of the aggregates returned by the query",
          "type": [
            "object",
            "null"
          ],
          "additionalProperties": true
        },
        "rows": {
          "description": "The rows returned by the query",
          "type": [
            "array",
            "null"
          ],
          "items": {
            "$ref": "#/definitions/Row"
          }
        }
      }
    }
  }
}